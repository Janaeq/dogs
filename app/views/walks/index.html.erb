<section class="hero is-link">
  <div class="hero-body">
    <p class="title">
        <%= walks_heading %>
    </p>
  </div>
</section>
<div class="message is-link">
<div class="message-body">
<% if @walks.empty? %>
<div class="container">
    <p class="subtitle"> No walks scheduled yet! </p>
     <%= link_to "Schedule a Walk!", new_walk_path, class: "button is-link is-outlined" %>
</div>
<% else %>
    <%= render partial: "dogindex" %>
<% end %>
</div>
</div>